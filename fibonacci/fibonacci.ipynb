   "source": [
    "def fib(n):\n",
    "\n",
    "    if n < 2:\n",
    "        return n\n",
    "    \n",
    "    a = 0\n",
    "    b = 1\n",
    "    c = 0\n",
    "    while n > 1:\n",
    "        c = a + b\n",
    "        a = b\n",
    "        b = c\n",
    "        n -= 1\n",
    "\n",
    "    return b"
   ]
  },
